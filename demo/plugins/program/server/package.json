{
  "name": "@project-sunbird/program",
  "version": "0.0.4",
  "description": "Program",
  "scripts": {
    "clean-dist-dev": "node_modules/.bin/rimraf ./../../../app/plugin-repo/@project-sunbird/program",
    "compile-dev": "tsc --p tsconfig-dev.json --noImplicitUseStrict",
    "copy-package-dev": "node_modules/.bin/cpy package.json ./../../../app/plugin-repo/@project-sunbird/program",
    "copy-db-dev": "node_modules/.bin/cpy db/* ./../../../app/plugin-repo/@project-sunbird/program/db",
    "resolve-dev-dependencies": "cd ./../../../app/plugin-repo/@project-sunbird/program && npm i --prod",
    "build-dev": "npm run clean-dist-dev &&  npm run compile-dev && npm run copy-package-dev && npm run copy-db-dev && npm run resolve-dev-dependencies",
    "copy-package-prod": "node ./utils/copy_package_json.js",
    "build-prod": "node_modules/.bin/rimraf ./dist && tsc --noImplicitUseStrict && npm run copy-package-prod && node_modules/.bin/cpy db/* ./dist/db",
    "test": "echo 'go to '../test' folder to execute test cases!'"
  },
  "engines": {
    "node": ">=7.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/project-sunbird/sunbird-ext-framework/tree/master/demo/plugins/program"
  },
  "keywords": [
    "sunbird",
    "Extensible framework",
    "plugins"
  ],
  "author": "Anoop HM<anoophm44@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "joi": "^13.4.0",
    "lodash": "^4.17.10",
    "rxjs": "^6.2.1"
  },
  "peerDependencies": {
    "@project-sunbird/ext-framework-server": "^0.1.6"
  },
  "devDependencies": {
    "cpy-cli": "^2.0.0",
    "rimraf": "^2.6.2",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.1",
    "standard": "^10.0.3",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1",
    "typescript-eslint-parser": "^13.0.0",
    "@project-sunbird/ext-framework-server": "^0.1.6"
  }
}
